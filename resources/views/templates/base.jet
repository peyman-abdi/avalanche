<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta content="width=device-width,initial-scale=1" name="viewport">
    {{ yield metas() }}
    {{ yield heads() }}
    <title>{{ yield title() }}</title>
  </head>
  <body {{ yield body_() }}>
    {{ yield body() }}
    <script src="{{ .App().AssetsUrl("js/base.js") }}" type="text/javascript"></script>
    {{ yield scripts() }}
  </body>
</html>